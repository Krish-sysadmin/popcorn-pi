+static int apm_dump_reg(int bus, unsigned char sa, unsigned char offset, int length)
+{
+	u32 data;
+	int ret = -1;
+	int pout;
+	long sstart, sstop, sstats;
+
+if (length > 4)
+	return -1;
+
+	sstart = ktime_to_ns(ktime_get());	
+	ret = i2c_sensor_read(bus, sa, offset, length, &data);
