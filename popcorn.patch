+int restore_thread_info(struct field_arch *arch, bool restore_segments)
+{
+	struct pt_regs *regs = current_pt_regs();
+	struct regset_riscv64 *regset = &arch->regs_riscv;
+	int cpu;
+
+	cpu = get_cpu();
+
+	regs->sepc = regset->pc;
+
+	regs->ra = regset->x[1];
+	regs->sp = regset->x[2];
+	regs->gp = regset->x[3];
+	//regs->tp = regset->x[4];
+	regs->t0 = regset->x[5];
+	regs->t1 = regset->x[6];
+	regs->t2 = regset->x[7];
+	regs->s0 = regset->x[8];
+	regs->s1 = regset->x[9];
+	regs->a0 = regset->x[10];
+	regs->a1 = regset->x[11];
+	regs->a2 = regset->x[12];
+	regs->a3 = regset->x[13];
+	regs->a4 = regset->x[14];
+	regs->a5 = regset->x[15];
+	regs->a6 = regset->x[16];
+	regs->a7 = regset->x[17];
+	regs->s2 = regset->x[18];
+	regs->s3 = regset->x[19];
+	regs->s4 = regset->x[20];
+	regs->s5 = regset->x[21];
+	regs->s6 = regset->x[22];
+	regs->s7 = regset->x[23];
+	regs->s8 = regset->x[24];
+	regs->s9 = regset->x[25];
+	regs->s10 = regset->x[26];
+	regs->s11 = regset->x[27];
+	regs->t3 = regset->x[28];
+	regs->t4 = regset->x[29];
+	regs->t5 = regset->x[30];
+	regs->t6 = regset->x[31];
+
+	if (restore_segments) {
+		regs->tp = arch->tls;
+		regs->sstatus = 0;
+		regs->sstatus |= SR_FS_INITIAL;
+		fstate_restore(current, regs);
+	}
+
+	put_cpu();
+
+#ifdef CONFIG_POPCORN_DEBUG_VERBOSE
+	PSPRINTK("%s [%d] tls %lx\n", __func__, current->pid, arch->tls);
+	PSPRINTK("%s [%d] ip %lx\n", __func__, current->pid,
+			regs->sepc);
+	PSPRINTK("%s [%d] sp %lx bp %lx\n", __func__, current->pid,
+			regs->sp, regs->s0);
+#endif
+
+	return 0;
+}
+
